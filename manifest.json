{
    "name": "my bookmark page",
    "version": "0.1",
    "description": "show your bookmark in start page!",
    "manifest_version": 2,
    "permissions": [
        "bookmarks"
    ],
    "chrome_settings_overrides" : {
        "homepage": "index.html"
    },
    "chrome_url_overrides" : {
        "newtab": "index.html"
    }
}
